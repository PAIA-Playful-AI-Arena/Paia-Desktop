{
  "name": "paia-desktop",
  "productName": "PAIA Desktop",
  "version": "2.3.0",
  "description": "A blockly application that can build AI to play games.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "reset-version": "npm --no-git-tag-version version 2.3.0",
    "package": "electron-forge package",
    "package-competition": "cross-var npm --no-git-tag-version version $npm_package_version-competition && electron-forge package && npm run reset-version",
    "package-competition-tn": "cross-var npm --no-git-tag-version version $npm_package_version-competition-tn && electron-forge package && npm run reset-version",
    "make": "electron-forge make",
    "make-competition": "cross-var npm --no-git-tag-version version $npm_package_version-competition && electron-forge make && npm run reset-version",
    "make-competition-tn": "cross-var npm --no-git-tag-version version $npm_package_version-competition-tn && electron-forge make && npm run reset-version",
    "make-only": "electron-forge make --skip-package",
    "make-py": "cd ./python && pyinstaller -y --hidden-import=cmath --hidden-import=csv --hidden-import=pathlib --hidden-import=matplotlib.pyplot --hidden-import=queue --hidden-import=pygame --hidden-import=multiprocessing --hidden-import=pickle --hidden-import=Box2D --hidden-import=sklearn.neighbors --hidden-import=sklearn.tree --hidden-import=sklearn.svm --hidden-import=sklearn.ensemble --hidden-import=sklearn.neural_network --hidden-import=sklearn.linear_model --hidden-import=sklearn.metrics --hidden-import=sklearn.model_selection --hidden-import=sklearn.utils._weight_vector --hidden-import=scipy.special.cython_special --hidden-import=pandas --hidden-import=pytmx interpreter.py"
  },
  "repository": "https://github.com/PAIA-Playful-AI-Arena/Paia-Desktop",
  "keywords": [
    "Blockly",
    "Python",
    "MLGame",
    "AI"
  ],
  "author": "PAIA",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.1.1"
  },
  "dependencies": {
    "@getstation/electron-google-oauth2": "^2.1.0",
    "@paia-arena/blockly": "^0.0.9",
    "about-window": "^1.15.1",
    "bootstrap": "^4.6.1",
    "bootstrap-icons": "^1.5.0",
    "codemirror": "^5.61.1",
    "cross-var": "^1.1.0",
    "dateformat": "^4.6.3",
    "dotenv": "^14.2.0",
    "download-git-repo": "^3.0.2",
    "electron-context-menu": "^3.1.2",
    "electron-installer-dmg": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1",
    "github-markdown-css": "^4.0.0",
    "googleapis": "^91.0.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "python-shell": "^2.0.3",
    "showdown": "^1.9.1",
    "uuid": "^8.3.2",
    "vex-js": "^4.1.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "media/paia-logo",
        "ignore": [
          "/.gitignore",
          "/.gitmodules",
          "/.vscode",
          "/requirements.txt",
          "/js/blocks",
          "/js/generators",
          "/js/i18n",
          "/js/mlgame",
          "/js/msg",
          "/js/build.py",
          "(.*)__pycache__",
          "/python/build",
          "/python/interpreter.spec",
          "/python/interpreter.py",
          "/venv"
        ],
        "asar": {
          "unpackDir": "{MLGame,python}"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "icon": "media/paia-logo.png",
              "bin": "PAIA Desktop"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "options": {
              "icon": "media/paia-logo.png",
              "bin": "PAIA Desktop"
            }
          }
        }
      ]
    }
  }
}
